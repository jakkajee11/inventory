    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.0",
    "prisma": "^5.9.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.4.0",
    "unplugin-swc": "^1.4.0",
    "vitest": "^1.2.0"
